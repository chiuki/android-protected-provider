<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.sqisland.android.protected_provider.main"
  android:versionCode="1"
  android:versionName="1.0" >

  <uses-sdk android:minSdkVersion="3" android:targetSdkVersion="12" />

  <supports-screens
    android:largeScreens="true"
    android:xlargeScreens="true" />

  <application android:label="@string/app_name" >
    <activity
      android:label="@string/app_name"
      android:name=".ProviderListActivity" >
      <intent-filter >
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".ProviderActivity" />
  </application>

  <uses-permission android:name="com.sqisland.android.protected_provider.ACCESS_DATA" />
</manifest>